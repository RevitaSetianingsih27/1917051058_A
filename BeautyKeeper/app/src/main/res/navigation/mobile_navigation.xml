<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.beautykeeper.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.beautykeeper.ui.search.SearchFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_category"
        android:name="com.example.beautykeeper.ui.category.CategoryFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_category" />

    <fragment
        android:id="@+id/navigation_register"
        android:name="com.example.beautykeeper.ui.register.RegisterFragment"
        android:label="Register"
        tools:layout="@layout/fragment_register" />
</navigation>